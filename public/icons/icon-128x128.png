<?xml version="1.0" encoding="UTF-8"?>
<svg width="128" height="128" viewBox="0 0 128 128" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <!-- Dark gradient background -->
    <linearGradient id="darkGrad128" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0D1B2A;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#1A1A1A;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0D1B2A;stop-opacity:1" />
    </linearGradient>
    
    <!-- Neon cyan glow -->
    <filter id="neonGlow128">
      <feGaussianBlur stdDeviation="4" result="coloredBlur"/>
      <feMerge>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
    
    <!-- Stronger glow for emphasis -->
    <filter id="strongGlow128">
      <feGaussianBlur stdDeviation="6" result="coloredBlur"/>
      <feMerge>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <!-- Background with rounded corners -->
  <rect width="128" height="128" rx="23.04" fill="url(#darkGrad128)"/>
  
  <!-- Outer neon border glow -->
  <rect x="6.4" y="6.4" width="115.2" height="115.2" 
        rx="19.2" fill="none" stroke="#00F0FF" stroke-width="1.024" 
        opacity="0.6" filter="url(#neonGlow128)"/>
  
  <!-- QR Code stylized element (top-left) -->
  <g transform="translate(19.2, 19.2)">
    <rect width="15.36" height="15.36" fill="#00F0FF" filter="url(#neonGlow128)"/>
    <rect x="5.12" y="5.12" width="5.12" height="5.12" fill="#0D1B2A"/>
  </g>
  
  <!-- QR Code stylized element (top-right) -->
  <g transform="translate(93.44, 19.2)">
    <rect width="15.36" height="15.36" fill="#00F0FF" filter="url(#neonGlow128)"/>
    <rect x="5.12" y="5.12" width="5.12" height="5.12" fill="#0D1B2A"/>
  </g>
  
  <!-- QR Code stylized element (bottom-left) -->
  <g transform="translate(19.2, 93.44)">
    <rect width="15.36" height="15.36" fill="#00F0FF" filter="url(#neonGlow128)"/>
    <rect x="5.12" y="5.12" width="5.12" height="5.12" fill="#0D1B2A"/>
  </g>
  
  <!-- Central dining element: Fork and Knife -->
  <g transform="translate(64, 64)" filter="url(#strongGlow128)">
    <!-- Plate circle -->
    <circle cx="0" cy="0" r="23.04" fill="none" stroke="#FF006E" stroke-width="1.92" opacity="0.8"/>
    
    <!-- Fork (left) -->
    <g transform="translate(-10.24, 0)">
      <rect x="-1.92" y="-15.36" width="3.84" height="30.72" rx="1.28" fill="#00F0FF"/>
      <rect x="-3.2" y="-15.36" width="1.92" height="10.24" fill="#00F0FF"/>
      <rect x="1.28" y="-15.36" width="1.92" height="10.24" fill="#00F0FF"/>
    </g>
    
    <!-- Knife (right) -->
    <g transform="translate(10.24, 0)">
      <rect x="-1.92" y="-15.36" width="3.84" height="30.72" rx="1.28" fill="#00F0FF"/>
      <path d="M -1.92 -15.36 L 1.92 -15.36 L 0 -19.2 Z" fill="#00F0FF"/>
    </g>
  </g>
  
  <!-- DQ monogram at bottom -->
  <text x="50%" y="112.64" font-family="Arial, sans-serif" font-size="19.2" 
        font-weight="bold" fill="#00F0FF" text-anchor="middle" filter="url(#neonGlow128)">DQ</text>
</svg>