<?xml version="1.0" encoding="UTF-8"?>
<svg width="96" height="96" viewBox="0 0 96 96" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <!-- Dark gradient background -->
    <linearGradient id="darkGrad96" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0D1B2A;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#1A1A1A;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0D1B2A;stop-opacity:1" />
    </linearGradient>
    
    <!-- Neon cyan glow -->
    <filter id="neonGlow96">
      <feGaussianBlur stdDeviation="4" result="coloredBlur"/>
      <feMerge>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
    
    <!-- Stronger glow for emphasis -->
    <filter id="strongGlow96">
      <feGaussianBlur stdDeviation="6" result="coloredBlur"/>
      <feMerge>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <!-- Background with rounded corners -->
  <rect width="96" height="96" rx="17.28" fill="url(#darkGrad96)"/>
  
  <!-- Outer neon border glow -->
  <rect x="4.800000000000001" y="4.800000000000001" width="86.4" height="86.4" 
        rx="14.399999999999999" fill="none" stroke="#00F0FF" stroke-width="0.768" 
        opacity="0.6" filter="url(#neonGlow96)"/>
  
  <!-- QR Code stylized element (top-left) -->
  <g transform="translate(14.399999999999999, 14.399999999999999)">
    <rect width="11.52" height="11.52" fill="#00F0FF" filter="url(#neonGlow96)"/>
    <rect x="3.84" y="3.84" width="3.84" height="3.84" fill="#0D1B2A"/>
  </g>
  
  <!-- QR Code stylized element (top-right) -->
  <g transform="translate(70.08, 14.399999999999999)">
    <rect width="11.52" height="11.52" fill="#00F0FF" filter="url(#neonGlow96)"/>
    <rect x="3.84" y="3.84" width="3.84" height="3.84" fill="#0D1B2A"/>
  </g>
  
  <!-- QR Code stylized element (bottom-left) -->
  <g transform="translate(14.399999999999999, 70.08)">
    <rect width="11.52" height="11.52" fill="#00F0FF" filter="url(#neonGlow96)"/>
    <rect x="3.84" y="3.84" width="3.84" height="3.84" fill="#0D1B2A"/>
  </g>
  
  <!-- Central dining element: Fork and Knife -->
  <g transform="translate(48, 48)" filter="url(#strongGlow96)">
    <!-- Plate circle -->
    <circle cx="0" cy="0" r="17.28" fill="none" stroke="#FF006E" stroke-width="1.44" opacity="0.8"/>
    
    <!-- Fork (left) -->
    <g transform="translate(-7.68, 0)">
      <rect x="-1.44" y="-11.52" width="2.88" height="23.04" rx="0.96" fill="#00F0FF"/>
      <rect x="-2.4000000000000004" y="-11.52" width="1.44" height="7.68" fill="#00F0FF"/>
      <rect x="0.96" y="-11.52" width="1.44" height="7.68" fill="#00F0FF"/>
    </g>
    
    <!-- Knife (right) -->
    <g transform="translate(7.68, 0)">
      <rect x="-1.44" y="-11.52" width="2.88" height="23.04" rx="0.96" fill="#00F0FF"/>
      <path d="M -1.44 -11.52 L 1.44 -11.52 L 0 -14.399999999999999 Z" fill="#00F0FF"/>
    </g>
  </g>
  
  <!-- DQ monogram at bottom -->
  <text x="50%" y="84.48" font-family="Arial, sans-serif" font-size="14.399999999999999" 
        font-weight="bold" fill="#00F0FF" text-anchor="middle" filter="url(#neonGlow96)">DQ</text>
</svg>