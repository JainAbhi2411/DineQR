<?xml version="1.0" encoding="UTF-8"?>
<svg width="72" height="72" viewBox="0 0 72 72" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <!-- Dark gradient background -->
    <linearGradient id="darkGrad72" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0D1B2A;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#1A1A1A;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0D1B2A;stop-opacity:1" />
    </linearGradient>
    
    <!-- Neon cyan glow -->
    <filter id="neonGlow72">
      <feGaussianBlur stdDeviation="4" result="coloredBlur"/>
      <feMerge>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
    
    <!-- Stronger glow for emphasis -->
    <filter id="strongGlow72">
      <feGaussianBlur stdDeviation="6" result="coloredBlur"/>
      <feMerge>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <!-- Background with rounded corners -->
  <rect width="72" height="72" rx="12.959999999999999" fill="url(#darkGrad72)"/>
  
  <!-- Outer neon border glow -->
  <rect x="3.6" y="3.6" width="64.8" height="64.8" 
        rx="10.799999999999999" fill="none" stroke="#00F0FF" stroke-width="0.5760000000000001" 
        opacity="0.6" filter="url(#neonGlow72)"/>
  
  <!-- QR Code stylized element (top-left) -->
  <g transform="translate(10.799999999999999, 10.799999999999999)">
    <rect width="8.64" height="8.64" fill="#00F0FF" filter="url(#neonGlow72)"/>
    <rect x="2.88" y="2.88" width="2.88" height="2.88" fill="#0D1B2A"/>
  </g>
  
  <!-- QR Code stylized element (top-right) -->
  <g transform="translate(52.56, 10.799999999999999)">
    <rect width="8.64" height="8.64" fill="#00F0FF" filter="url(#neonGlow72)"/>
    <rect x="2.88" y="2.88" width="2.88" height="2.88" fill="#0D1B2A"/>
  </g>
  
  <!-- QR Code stylized element (bottom-left) -->
  <g transform="translate(10.799999999999999, 52.56)">
    <rect width="8.64" height="8.64" fill="#00F0FF" filter="url(#neonGlow72)"/>
    <rect x="2.88" y="2.88" width="2.88" height="2.88" fill="#0D1B2A"/>
  </g>
  
  <!-- Central dining element: Fork and Knife -->
  <g transform="translate(36, 36)" filter="url(#strongGlow72)">
    <!-- Plate circle -->
    <circle cx="0" cy="0" r="12.959999999999999" fill="none" stroke="#FF006E" stroke-width="1.08" opacity="0.8"/>
    
    <!-- Fork (left) -->
    <g transform="translate(-5.76, 0)">
      <rect x="-1.08" y="-8.64" width="2.16" height="17.28" rx="0.72" fill="#00F0FF"/>
      <rect x="-1.8" y="-8.64" width="1.08" height="5.76" fill="#00F0FF"/>
      <rect x="0.72" y="-8.64" width="1.08" height="5.76" fill="#00F0FF"/>
    </g>
    
    <!-- Knife (right) -->
    <g transform="translate(5.76, 0)">
      <rect x="-1.08" y="-8.64" width="2.16" height="17.28" rx="0.72" fill="#00F0FF"/>
      <path d="M -1.08 -8.64 L 1.08 -8.64 L 0 -10.799999999999999 Z" fill="#00F0FF"/>
    </g>
  </g>
  
  <!-- DQ monogram at bottom -->
  <text x="50%" y="63.36" font-family="Arial, sans-serif" font-size="10.799999999999999" 
        font-weight="bold" fill="#00F0FF" text-anchor="middle" filter="url(#neonGlow72)">DQ</text>
</svg>