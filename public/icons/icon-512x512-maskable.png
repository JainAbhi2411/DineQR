<?xml version="1.0" encoding="UTF-8"?>
<svg width="512" height="512" viewBox="0 0 512 512" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <!-- Dark gradient background -->
    <linearGradient id="darkGrad512" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0D1B2A;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#1A1A1A;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0D1B2A;stop-opacity:1" />
    </linearGradient>
    
    <!-- Neon cyan glow -->
    <filter id="neonGlow512">
      <feGaussianBlur stdDeviation="4" result="coloredBlur"/>
      <feMerge>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
    
    <!-- Stronger glow for emphasis -->
    <filter id="strongGlow512">
      <feGaussianBlur stdDeviation="6" result="coloredBlur"/>
      <feMerge>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <!-- Background with rounded corners -->
  <rect width="512" height="512" rx="92.16" fill="url(#darkGrad512)"/>
  
  <!-- Outer neon border glow -->
  <rect x="25.6" y="25.6" width="460.8" height="460.8" 
        rx="76.8" fill="none" stroke="#00F0FF" stroke-width="4.096" 
        opacity="0.6" filter="url(#neonGlow512)"/>
  
  <!-- QR Code stylized element (top-left) -->
  <g transform="translate(112.64, 112.64)">
    <rect width="49.152" height="49.152" fill="#00F0FF" filter="url(#neonGlow512)"/>
    <rect x="16.384" y="16.384" width="16.384" height="16.384" fill="#0D1B2A"/>
  </g>
  
  <!-- QR Code stylized element (top-right) -->
  <g transform="translate(350.20799999999997, 112.64)">
    <rect width="49.152" height="49.152" fill="#00F0FF" filter="url(#neonGlow512)"/>
    <rect x="16.384" y="16.384" width="16.384" height="16.384" fill="#0D1B2A"/>
  </g>
  
  <!-- QR Code stylized element (bottom-left) -->
  <g transform="translate(112.64, 350.20799999999997)">
    <rect width="49.152" height="49.152" fill="#00F0FF" filter="url(#neonGlow512)"/>
    <rect x="16.384" y="16.384" width="16.384" height="16.384" fill="#0D1B2A"/>
  </g>
  
  <!-- Central dining element: Fork and Knife -->
  <g transform="translate(256, 256)" filter="url(#strongGlow512)">
    <!-- Plate circle -->
    <circle cx="0" cy="0" r="73.728" fill="none" stroke="#FF006E" stroke-width="6.144" opacity="0.8"/>
    
    <!-- Fork (left) -->
    <g transform="translate(-32.768, 0)">
      <rect x="-6.144" y="-49.152" width="12.288" height="98.304" rx="4.096" fill="#00F0FF"/>
      <rect x="-10.240000000000002" y="-49.152" width="6.144" height="32.768" fill="#00F0FF"/>
      <rect x="4.096" y="-49.152" width="6.144" height="32.768" fill="#00F0FF"/>
    </g>
    
    <!-- Knife (right) -->
    <g transform="translate(32.768, 0)">
      <rect x="-6.144" y="-49.152" width="12.288" height="98.304" rx="4.096" fill="#00F0FF"/>
      <path d="M -6.144 -49.152 L 6.144 -49.152 L 0 -61.44 Z" fill="#00F0FF"/>
    </g>
  </g>
  
  <!-- DQ monogram at bottom -->
  <text x="50%" y="450.56" font-family="Arial, sans-serif" font-size="61.44" 
        font-weight="bold" fill="#00F0FF" text-anchor="middle" filter="url(#neonGlow512)">DQ</text>
</svg>