<?xml version="1.0" encoding="UTF-8"?>
<svg width="384" height="384" viewBox="0 0 384 384" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <!-- Dark gradient background -->
    <linearGradient id="darkGrad384" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0D1B2A;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#1A1A1A;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0D1B2A;stop-opacity:1" />
    </linearGradient>
    
    <!-- Neon cyan glow -->
    <filter id="neonGlow384">
      <feGaussianBlur stdDeviation="4" result="coloredBlur"/>
      <feMerge>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
    
    <!-- Stronger glow for emphasis -->
    <filter id="strongGlow384">
      <feGaussianBlur stdDeviation="6" result="coloredBlur"/>
      <feMerge>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <!-- Background with rounded corners -->
  <rect width="384" height="384" rx="69.12" fill="url(#darkGrad384)"/>
  
  <!-- Outer neon border glow -->
  <rect x="19.200000000000003" y="19.200000000000003" width="345.6" height="345.6" 
        rx="57.599999999999994" fill="none" stroke="#00F0FF" stroke-width="3.072" 
        opacity="0.6" filter="url(#neonGlow384)"/>
  
  <!-- QR Code stylized element (top-left) -->
  <g transform="translate(57.599999999999994, 57.599999999999994)">
    <rect width="46.08" height="46.08" fill="#00F0FF" filter="url(#neonGlow384)"/>
    <rect x="15.36" y="15.36" width="15.36" height="15.36" fill="#0D1B2A"/>
  </g>
  
  <!-- QR Code stylized element (top-right) -->
  <g transform="translate(280.32, 57.599999999999994)">
    <rect width="46.08" height="46.08" fill="#00F0FF" filter="url(#neonGlow384)"/>
    <rect x="15.36" y="15.36" width="15.36" height="15.36" fill="#0D1B2A"/>
  </g>
  
  <!-- QR Code stylized element (bottom-left) -->
  <g transform="translate(57.599999999999994, 280.32)">
    <rect width="46.08" height="46.08" fill="#00F0FF" filter="url(#neonGlow384)"/>
    <rect x="15.36" y="15.36" width="15.36" height="15.36" fill="#0D1B2A"/>
  </g>
  
  <!-- Central dining element: Fork and Knife -->
  <g transform="translate(192, 192)" filter="url(#strongGlow384)">
    <!-- Plate circle -->
    <circle cx="0" cy="0" r="69.12" fill="none" stroke="#FF006E" stroke-width="5.76" opacity="0.8"/>
    
    <!-- Fork (left) -->
    <g transform="translate(-30.72, 0)">
      <rect x="-5.76" y="-46.08" width="11.52" height="92.16" rx="3.84" fill="#00F0FF"/>
      <rect x="-9.600000000000001" y="-46.08" width="5.76" height="30.72" fill="#00F0FF"/>
      <rect x="3.84" y="-46.08" width="5.76" height="30.72" fill="#00F0FF"/>
    </g>
    
    <!-- Knife (right) -->
    <g transform="translate(30.72, 0)">
      <rect x="-5.76" y="-46.08" width="11.52" height="92.16" rx="3.84" fill="#00F0FF"/>
      <path d="M -5.76 -46.08 L 5.76 -46.08 L 0 -57.599999999999994 Z" fill="#00F0FF"/>
    </g>
  </g>
  
  <!-- DQ monogram at bottom -->
  <text x="50%" y="337.92" font-family="Arial, sans-serif" font-size="57.599999999999994" 
        font-weight="bold" fill="#00F0FF" text-anchor="middle" filter="url(#neonGlow384)">DQ</text>
</svg>