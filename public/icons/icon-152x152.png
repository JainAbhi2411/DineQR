<?xml version="1.0" encoding="UTF-8"?>
<svg width="152" height="152" viewBox="0 0 152 152" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <!-- Dark gradient background -->
    <linearGradient id="darkGrad152" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0D1B2A;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#1A1A1A;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0D1B2A;stop-opacity:1" />
    </linearGradient>
    
    <!-- Neon cyan glow -->
    <filter id="neonGlow152">
      <feGaussianBlur stdDeviation="4" result="coloredBlur"/>
      <feMerge>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
    
    <!-- Stronger glow for emphasis -->
    <filter id="strongGlow152">
      <feGaussianBlur stdDeviation="6" result="coloredBlur"/>
      <feMerge>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <!-- Background with rounded corners -->
  <rect width="152" height="152" rx="27.36" fill="url(#darkGrad152)"/>
  
  <!-- Outer neon border glow -->
  <rect x="7.6000000000000005" y="7.6000000000000005" width="136.8" height="136.8" 
        rx="22.8" fill="none" stroke="#00F0FF" stroke-width="1.216" 
        opacity="0.6" filter="url(#neonGlow152)"/>
  
  <!-- QR Code stylized element (top-left) -->
  <g transform="translate(22.8, 22.8)">
    <rect width="18.24" height="18.24" fill="#00F0FF" filter="url(#neonGlow152)"/>
    <rect x="6.08" y="6.08" width="6.08" height="6.08" fill="#0D1B2A"/>
  </g>
  
  <!-- QR Code stylized element (top-right) -->
  <g transform="translate(110.96, 22.8)">
    <rect width="18.24" height="18.24" fill="#00F0FF" filter="url(#neonGlow152)"/>
    <rect x="6.08" y="6.08" width="6.08" height="6.08" fill="#0D1B2A"/>
  </g>
  
  <!-- QR Code stylized element (bottom-left) -->
  <g transform="translate(22.8, 110.96)">
    <rect width="18.24" height="18.24" fill="#00F0FF" filter="url(#neonGlow152)"/>
    <rect x="6.08" y="6.08" width="6.08" height="6.08" fill="#0D1B2A"/>
  </g>
  
  <!-- Central dining element: Fork and Knife -->
  <g transform="translate(76, 76)" filter="url(#strongGlow152)">
    <!-- Plate circle -->
    <circle cx="0" cy="0" r="27.36" fill="none" stroke="#FF006E" stroke-width="2.28" opacity="0.8"/>
    
    <!-- Fork (left) -->
    <g transform="translate(-12.16, 0)">
      <rect x="-2.28" y="-18.24" width="4.56" height="36.48" rx="1.52" fill="#00F0FF"/>
      <rect x="-3.8000000000000003" y="-18.24" width="2.28" height="12.16" fill="#00F0FF"/>
      <rect x="1.52" y="-18.24" width="2.28" height="12.16" fill="#00F0FF"/>
    </g>
    
    <!-- Knife (right) -->
    <g transform="translate(12.16, 0)">
      <rect x="-2.28" y="-18.24" width="4.56" height="36.48" rx="1.52" fill="#00F0FF"/>
      <path d="M -2.28 -18.24 L 2.28 -18.24 L 0 -22.8 Z" fill="#00F0FF"/>
    </g>
  </g>
  
  <!-- DQ monogram at bottom -->
  <text x="50%" y="133.76" font-family="Arial, sans-serif" font-size="22.8" 
        font-weight="bold" fill="#00F0FF" text-anchor="middle" filter="url(#neonGlow152)">DQ</text>
</svg>