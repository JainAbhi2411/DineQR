<?xml version="1.0" encoding="UTF-8"?>
<svg width="144" height="144" viewBox="0 0 144 144" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <!-- Dark gradient background -->
    <linearGradient id="darkGrad144" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0D1B2A;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#1A1A1A;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0D1B2A;stop-opacity:1" />
    </linearGradient>
    
    <!-- Neon cyan glow -->
    <filter id="neonGlow144">
      <feGaussianBlur stdDeviation="4" result="coloredBlur"/>
      <feMerge>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
    
    <!-- Stronger glow for emphasis -->
    <filter id="strongGlow144">
      <feGaussianBlur stdDeviation="6" result="coloredBlur"/>
      <feMerge>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <!-- Background with rounded corners -->
  <rect width="144" height="144" rx="25.919999999999998" fill="url(#darkGrad144)"/>
  
  <!-- Outer neon border glow -->
  <rect x="7.2" y="7.2" width="129.6" height="129.6" 
        rx="21.599999999999998" fill="none" stroke="#00F0FF" stroke-width="1.1520000000000001" 
        opacity="0.6" filter="url(#neonGlow144)"/>
  
  <!-- QR Code stylized element (top-left) -->
  <g transform="translate(21.599999999999998, 21.599999999999998)">
    <rect width="17.28" height="17.28" fill="#00F0FF" filter="url(#neonGlow144)"/>
    <rect x="5.76" y="5.76" width="5.76" height="5.76" fill="#0D1B2A"/>
  </g>
  
  <!-- QR Code stylized element (top-right) -->
  <g transform="translate(105.12, 21.599999999999998)">
    <rect width="17.28" height="17.28" fill="#00F0FF" filter="url(#neonGlow144)"/>
    <rect x="5.76" y="5.76" width="5.76" height="5.76" fill="#0D1B2A"/>
  </g>
  
  <!-- QR Code stylized element (bottom-left) -->
  <g transform="translate(21.599999999999998, 105.12)">
    <rect width="17.28" height="17.28" fill="#00F0FF" filter="url(#neonGlow144)"/>
    <rect x="5.76" y="5.76" width="5.76" height="5.76" fill="#0D1B2A"/>
  </g>
  
  <!-- Central dining element: Fork and Knife -->
  <g transform="translate(72, 72)" filter="url(#strongGlow144)">
    <!-- Plate circle -->
    <circle cx="0" cy="0" r="25.919999999999998" fill="none" stroke="#FF006E" stroke-width="2.16" opacity="0.8"/>
    
    <!-- Fork (left) -->
    <g transform="translate(-11.52, 0)">
      <rect x="-2.16" y="-17.28" width="4.32" height="34.56" rx="1.44" fill="#00F0FF"/>
      <rect x="-3.6" y="-17.28" width="2.16" height="11.52" fill="#00F0FF"/>
      <rect x="1.44" y="-17.28" width="2.16" height="11.52" fill="#00F0FF"/>
    </g>
    
    <!-- Knife (right) -->
    <g transform="translate(11.52, 0)">
      <rect x="-2.16" y="-17.28" width="4.32" height="34.56" rx="1.44" fill="#00F0FF"/>
      <path d="M -2.16 -17.28 L 2.16 -17.28 L 0 -21.599999999999998 Z" fill="#00F0FF"/>
    </g>
  </g>
  
  <!-- DQ monogram at bottom -->
  <text x="50%" y="126.72" font-family="Arial, sans-serif" font-size="21.599999999999998" 
        font-weight="bold" fill="#00F0FF" text-anchor="middle" filter="url(#neonGlow144)">DQ</text>
</svg>